class Animal{
    String name;
    Animal(String name){
        this.name = name;
    }
    public static void bark(){
        System.out.println("bark");
    }
}
class Dog extends Animal{
    Dog(String name){
        super(name);
    }
    public static void bark() {
        System.out.println("Wang");
    }
}
class Cat extends Animal{
    Cat(String name){
        super(name);
    }
    public static void bark() {
        System.out.println("Miao");
    }
}
public class Main {
    public static void main(String[] args) {
        Animal animal1 = new Animal("Kitty");
        animal1.bark();
        Dog dog1 = new Dog("Luccy");
        dog1.bark();
        Cat cat1 = new Cat("Emily");
        cat1.bark();
    }
}
